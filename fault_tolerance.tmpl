        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
            <h1 class="h2">Cluster overview</h1>

            <!-- Error message -->
            <div id="instance-error-msg" class="alert alert-danger alert-dismissible fade" role="alert">
              <strong>Error</strong> Message.
            </div>

          </div>

          <table id="instance-table">
            <thead>
              <tr>
                <th data-field="0" data-sortable="true" data-formatter="instanceFormatter">Instance address</th>
                <th data-field="1" data-sortable="true" data-formatter="booleanFormatter">Healthy</th>
                <th data-field="2" data-sortable="true" data-formatter="durationFormatter">Time since heartbeat [sec]</th>
                <th data-field="3" data-sortable="false" data-formatter="domainsFormatter">Domains</th>
                <th data-field="4" data-sortable="false" data-formatter="shutdownFormatter">Shutdown</th>
              </tr>
            </thead>
          </table>
        </main>

        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
            <h1 class="h2">
              Domain overview
              <button type="button" class="btn btn-outline-secondary" onclick="update_domains()">
                <span data-feather="refresh-cw"></span>
              </button>
            </h1>

            <!-- Error message -->
            <div id="domain-error-msg" class="alert alert-danger alert-dismissible fade" role="alert">
              <strong>Error</strong> Message.
            </div>

          </div>

          <table id="domain-table">
            <thead>
              <tr>
                <th data-field="0" data-sortable="true">Domain</th>
                <th data-field="1" data-sortable="true">Nodes</th>
                <th data-field="2" data-sortable="false">Min Label</th>
                <th data-field="3" data-sortable="false">Log Size</th>
                <th data-field="4" data-sortable="false">Provenance</th>
              </tr>
            </thead>
          </table>
        </main>

        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
            <h1 class="h2">Read-write</h1>
          </div>
          <p><button type="button" class="btn btn-outline-secondary" onclick="build_table()">Build Table</button></p>
          <p><button type="button" class="btn btn-outline-secondary" onclick="build_view()">Build View</button></p>
        </main>
